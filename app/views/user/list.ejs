<!-- User List Table -->
<div class="container py-2 my-3 rounded bg-light">
    <%- include('./new_button') %>
</div>

<div class="container" id="user-list">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">First</th>
                <th scope="col">Last</th>
                <th scope="col">Handle</th>
                <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
            <% userList.forEach(function(user, index) { %>
                <%- include('row', { user: user, index: index }) %>
            <% }) %>
        </tbody>
    </table>
</div>

<%- include('./form') %>
