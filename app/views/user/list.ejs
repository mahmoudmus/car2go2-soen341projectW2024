<div class="container py-2 my-3 rounded bg-light d-flex justify-content-end">
    <%- include('./newButton') %>
</div>

<div class="container" id="user-list">
    <table class="table">
        <thead>
            <tr>
                <th scope="col" class="d-none d-sm-table-cell">Name</th>
                <th scope="col">Email</th>
                <th scope="col" class="d-none d-md-table-cell">Address</th>
                <th scope="col" class="d-none d-sm-table-cell">Type</th>
                <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
            <% userList.forEach(function(user) { %>
                <%- include('row', { user: user }) %>
            <% }) %>
        </tbody>
    </table>
</div>

